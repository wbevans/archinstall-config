{
  "audio": "pipewire",
  "bootloader": "grub-install",
  "custom-commands": [
    "usermod -aG docker evans",
    "git clone https://aur.archlinux.org/yay.git /home/evans/yay",
    "cd /home/evans/yay",
    "makepkg -is",
    "cd /home/evans",
    "rm -rf /home/evans/yay",
    "yay --needed --nocleanmenu --nodiffmenu -S visual-studio-code-bin ttf-meslo-nerd-font-powerlevel10k oh-my-zsh-git zsh-autosuggestions-git zsh-syntax-highlighting-git zsh-theme-powerlevel10k-git ppd-dell-e525w"
  ],
  "filesystem": "btrfs",
  "gfx_driver": "All open-source (default)",
  "harddrives": [
    "/dev/sda"
  ],
  "hostname": "archy",
  "kernels": [
    "linux-zen"
  ],
  "keyboard-language": "us",
  "mirror-region": "United States",
  "nic": {
    "NetworkManager": true
  },
  "ntp": true,
  "packages": [
    "man-db",
    "man-pages",
    "plasma-desktop",
    "plasma-wayland-session",
    "wireplumber",
    "phonon-qt5-gstreamer",
    "gnu-free-fonts",
    "pipewire-pulse",
    "pipewire-alsa",
    "pipewire-jack",
    "plasma-pa sddm",
    "sddm-kcm",
    "konsole",
    "dolphin",
    "firefox",
    "kate",
    "vim",
    "breeze-gtk",
    "kde-gtk-config",
    "kdeplasma-addons",
    "plasma-nm",
    "vlc",
    "digikam",
    "ark",
    "kinfocenter",
    "kwalletmanager",
    "gwenview",
    "kipi-plugins",
    "spectacle",
    "okular",
    "discover",
    "base-devel",
    "git",
    "go",
    "zsh",
    "krita",
    "kdenlive"
],
  "profile": "minimal",
  "services": [
    "docker.service",
    "sddm.service"
  ],
  "swap": true,
  "sys-encoding": "utf-8",
  "sys-language": "en_US",
  "timezone": "America/Chicago"
}
