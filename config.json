{
  "audio": "pipewire",
  "bootloader": "grub-install",
  "custom-commands": [
    "usermod -aG docker evans",
    "git clone https://aur.archlinux.org/yay.git /home/evans/yay",
    "cd /home/evans/yay",
    "makepkg -is",
    "cd /home/evans",
    "rm -rf /home/evans/yay",
    "yay --needed --nocleanmenu --nodiffmenu -S visual-studio-code-bin ttf-meslo-nerd-font-powerlevel10k oh-my-zsh-git zsh-autosuggestions-git zsh-syntax-highlighting-git zsh-theme-powerlevel10k-git ppd-dell-e525w"
  ],
  "filesystem": "btrfs",
  "gfx_driver": "VMware / VirtualBox (open-source)",
  "harddrives": [
    "/dev/sda"
  ],
  "hostname": "archy",
  "kernels": [
    "linux-zen"
  ],
  "keyboard-language": "us",
  "mirror-region": "United States",
  "nic": {
    "NetworkManager": true
  },
  "ntp": true,
  "packages": [
    "man-db",
    "man-pages"
],
  "profile": "minimal",
  "services": [
    "docker.service"
  ],
  "swap": true,
  "sys-encoding": "utf-8",
  "sys-language": "en_US",
  "timezone": "America/Chicago"
}
